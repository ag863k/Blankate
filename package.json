{
  "name": "blankate",
  "version": "1.0.0",
  "private": true,
  "description": "Modern crypto & Web3 dashboard with real-time data and portfolio tracking",
  "keywords": ["crypto", "web3", "blockchain", "defi", "nft", "ethereum", "dashboard"],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:netlify": "cross-env NETLIFY=true next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "echo \"Tests not implemented yet\" && exit 0",
    "clean": "rimraf .next out dist build .turbo",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "docker:build": "docker build -t blankate .",
    "docker:run": "docker run -p 3000:3000 blankate",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@headlessui/react": "^2.0.4",
    "@heroicons/react": "^2.1.3",
    "@rainbow-me/rainbowkit": "^2.1.2",
    "@tanstack/react-query": "^5.45.1",
    "clsx": "^2.1.1",
    "framer-motion": "^11.2.10",
    "next": "^14.2.3",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.12.7",
    "swr": "^2.2.5",
    "viem": "^2.13.6",
    "wagmi": "^2.9.8"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.3",
    "husky": "^9.0.11",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.10",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
